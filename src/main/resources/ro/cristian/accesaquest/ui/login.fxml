<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<GridPane alignment="CENTER" centerShape="false" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="450.0" prefWidth="900.0" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.cristian.accesaquest.controllers.Login">
    <columnConstraints>
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
    </columnConstraints>
    <rowConstraints>
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
    </rowConstraints>
    <VBox alignment="TOP_CENTER" fillWidth="false" prefHeight="150.0" prefWidth="200.0" spacing="10.0" GridPane.columnIndex="1" GridPane.halignment="CENTER" GridPane.rowIndex="1" GridPane.valignment="CENTER">
        <Label alignment="CENTER" prefHeight="30.0" prefWidth="300.0" text="Email" textAlignment="CENTER">
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </Label>
        <TextField fx:id="emailField" alignment="CENTER" prefHeight="30.0" prefWidth="300.0">
            <VBox.margin>
                <Insets bottom="10.0" />
            </VBox.margin>
        </TextField>
        <Label alignment="CENTER" prefHeight="30.0" prefWidth="300.0" text="Password" />
        <PasswordField fx:id="passwordField" alignment="CENTER" prefHeight="30.0" prefWidth="300.0" />
        <Button mnemonicParsing="false" onAction="#login" prefHeight="30.0" prefWidth="300.0" text="Log in">
            <VBox.margin>
                <Insets top="20.0" />
            </VBox.margin>
        </Button>
    </VBox>
</GridPane>
